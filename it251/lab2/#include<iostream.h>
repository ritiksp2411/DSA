#include<iostream.h>
using namespace std;
class incre
{
	int i,j;
	public :
	incre(int i=0)
	{
	this->i+i;

	}
	incre operator++()
	{
	incre temp;
	temp.i=++i;
	return temp;
	}
	void display()
	{
	cout<<"i="<<i<<"j"<<j<<endl;

	}
	incre operator--()
	{
	incre s;
	s.i=--i;
	return s;
	}
	void display()

	{
	cout<<"i="<<i<<"j"<<j<<endl;
	}
	void get_in(int x,int y)
	{
	x=i;
	y=j;

	};
	void main()
{
	
	incre s;
	s.get_in(2,3);
	s.display();
	}



