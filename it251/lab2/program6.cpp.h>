#include<iostream.h>
using namespace std;

class incre
{
	int i,j;
	public:
	invre(i=0);
	incre operator++()
	{
	incre temp;
	temp.i=++i;
	return temp;
	}
	void display()
	{
	cout<<i<<"+j"<<j<<endl;
	}
	incre operator--()
	{
	incre t;
	t.i=--i;
	return t;

	}
	void display()
	{
	cout<<i<<"+j"<<j<<endl;
	}
	void put_in(int x,int y)
	{
	x=i;
	y=j;
	}

};
void main()
{
	incre s;
	s.put_in(2,2)
	s operator++(i)
	
	++i;
	s.display();
	s.operator--(i);
	--i;
	s.display();
	}